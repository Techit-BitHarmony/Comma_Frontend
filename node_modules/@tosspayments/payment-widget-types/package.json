{
  "name": "@tosspayments/payment-widget-types",
  "version": "0.0.5",
  "description": "Types used in PaymentWidget SDK",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "lint": "yarn run -T eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "yarn lint --fix",
    "typecheck": "tsc --noEmit",
    "build": "tsc -p tsconfig.json && tsc -p tsconfig.esm.json",
    "prepack": "yarn build",
    "generate-types": "echo \"no generate-types specified.\""
  },
  "files": [
    "dist",
    "esm"
  ],
  "devDependencies": {
    "@babel/runtime": "^7.17.2",
    "@tosspayments/payment__types": "^workspace:*",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "typescript": "^4.7.4"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "module": "esm/index.js"
  },
  "module": "esm/index.js",
  "types": "dist/index.d.ts"
}